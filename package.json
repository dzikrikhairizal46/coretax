  "license": "MIT",
  "author": {
    "name": "CoreTax Team",
    "email": "coretax@example.com",
    "url": "https://coretax.id"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/coretax.git",
    "directory": "/"
  },
  "homepage": "https://coretax.id",
  "bugs": {
    "url": "https://github.com/yourusername/coretax/issues",
